{
  parserClass="com.aymanosman.racket.plugin.parser.RacketParser"

  extends="com.intellij.extapi.psi.ASTWrapperPsiElement"

  psiClassPrefix="Racket"
  psiImplClassSuffix="Impl"
  psiPackage="com.aymanosman.racket.plugin.psi"
  psiImplPackage="com.aymanosman.racket.plugin.psi.impl"

  elementTypeHolderClass="com.aymanosman.racket.plugin.psi.RacketTypes"
  elementTypeClass="com.aymanosman.racket.plugin.psi.RacketElementType"
  tokenTypeClass="com.aymanosman.racket.plugin.psi.RacketTokenType"

  psiImplUtilClass="com.aymanosman.racket.plugin.psi.impl.RacketPsiImplUtil"
}

RacketFile ::= HASH_LANG Item*

Item ::= (Form | COMMENT | SEXP_COMMENT | CRLF)

Form ::= Datum
       | DOT
       | CHARACTER
       | NUMBER
       | BOOLEAN
       | STRING
       | IDENTIFIER
       | KEYWORD
       | OPEN_PAREN Item* CLOSE_PAREN
       | OPEN_SQUARE Item* CLOSE_SQUARE
       // | OPEN_BRACE Item* CLOSE_BRACE

Datum ::= CONSTANT Form
        | UNQUOTE Form
